{
    "name": "angular4-seed",
    "version": "1.0.0",
    "scripts": {
        "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
        "clean": "npm cache clean && npm run rimraf node_modules dist",
        "e2e": "protractor protractor.conf.js",
        "postinstall": "npm run webdriver:update",
        "rimraf": "rimraf",
        "start": "webpack-dev-server --inline --progress --port 3000",
        "test": "karma start",
        "webdriver:update": "webdriver-manager update"
    },
    "keywords": [
        "angular4",
        "webpack",
        "karma",
        "protractor",
        "typescript"
    ],
    "author": "Justin Emmerich",
    "homepage": "https://github.com/Justinemmerich/angular4-seed",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Justinemmerich/angular4-seed.git"
    },
    "bugs": {
        "url": "https://github.com/Justinemmerich/angular4-seed/issues"
    },
    "dependencies": {
        "@angular/common": "4.0.3",
        "@angular/compiler": "4.0.3",
        "@angular/core": "4.0.3",
        "@angular/forms": "4.0.3",
        "@angular/http": "4.0.3",
        "@angular/platform-browser": "4.0.3",
        "@angular/platform-browser-dynamic": "4.0.3",
        "@angular/router": "4.0.3",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.24",
        "@ngx-translate/core": "6.0.1",
        "@ngx-translate/http-loader": "0.0.3",
        "bootstrap": "4.0.0-alpha.6",
        "core-js": "2.4.1",
        "rxjs": "5.3.0",
        "zone.js": "0.8.8"
    },
    "devDependencies": {
        "@types/core-js": "0.9.41",
        "@types/jasmine": "2.5.47",
        "@types/node": "7.0.13",
        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "3.1.2",
        "copy-webpack-plugin": "4.0.1",
        "css-loader": "0.28.0",
        "extract-text-webpack-plugin": "2.1.0",
        "file-loader": "0.11.1",
        "html-loader": "0.4.5",
        "html-webpack-plugin": "2.28.0",
        "jasmine-core": "2.5.2",
        "json-loader": "0.5.4",
        "karma": "1.6.0",
        "karma-coverage": "^1.1.1",
        "karma-jasmine": "1.1.0",
        "karma-mocha-reporter": "2.2.3",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-remap-coverage": "^0.1.4",
        "karma-sourcemap-loader": "0.3.7",
        "karma-webpack": "2.0.3",
        "node-sass": "4.5.2",
        "phantomjs-prebuilt": "^2.1.14",
        "raw-loader": "0.5.1",
        "rimraf": "^2.6.1",
        "sass-loader": "6.0.3",
        "ts-helpers": "1.1.2",
        "typescript": "2.2.2",
        "webpack": "2.4.1",
        "webpack-dev-server": "2.4.4",
        "webpack-merge": "4.1.0"
    }
